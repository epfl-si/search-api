{
  "name": "search-api",
  "private": true,
  "version": "0.0.1",
  "description": "EPFL Search Engine API",
  "repository": "epfl-si/search-api",
  "author": "William Belle <william.belle@gmail.com>",
  "homepage": "https://github.com/epfl-si/search-api#readme",
  "engines": {
    "node": "^14.17.0 || >=16.0.0"
  },
  "scripts": {
    "docs": "jsdoc -d docs/ src/**",
    "lint": "eslint src --ext=js",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --check --ignore-unknown '**' '!**/*.js' '!**/ansible-deps-cache'",
    "prettier:fix": "prettier --write --ignore-unknown '**' '!**/*.js' '!**/ansible-deps-cache'",
    "start": "nodemon src/server.js",
    "test": "npm run lint && jest --forceExit",
    "test:coverage": "jest --coverage --forceExit",
    "test:watch": "jest --watchAll"
  },
  "devDependencies": {
    "eslint": "^8.37.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.5.0",
    "jsdoc": "^4.0.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "supertest": "^6.3.3"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "googleapis": "^114.0.0",
    "helmet": "^7.0.0",
    "ldapjs": "^3.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.3.3",
    "node-cache": "^5.1.2"
  }
}
