{
  "name": "search-api",
  "private": true,
  "version": "2.2.0",
  "description": "EPFL Search Engine API",
  "repository": "epfl-si/search-api",
  "author": "William Belle <william.belle@gmail.com>",
  "homepage": "https://github.com/epfl-si/search-api#readme",
  "engines": {
    "node": ">= 20.12"
  },
  "scripts": {
    "docs": "jsdoc -c jsdoc.json",
    "inspect": "nodemon --inspect src/server.js",
    "lint": "eslint src tests .jest --ext=js",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --check --ignore-unknown '**' '!**/*.js' '!**/ansible-deps-cache'",
    "prettier:fix": "prettier --write --ignore-unknown '**' '!**/*.js' '!**/ansible-deps-cache'",
    "start": "nodemon src/server.js",
    "test": "npm run lint && jest --verbose",
    "test:coverage": "jest --coverage --verbose",
    "test:watch": "jest --watchAll --verbose"
  },
  "devDependencies": {
    "docdash": "^2.0.1",
    "eslint": "^9.23.0",
    "eslint-plugin-jest": "^29.0.1",
    "jest": "^30.0.0",
    "jsdoc": "^4.0.2",
    "neostandard": "^0.12.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.1",
    "supertest": "^7.0.0"
  },
  "dependencies": {
    "axios": "^1.8.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-prom-bundle": "^8.0.0",
    "express-session": "^1.18.1",
    "googleapis": "^150.0.1",
    "helmet": "^8.1.0",
    "ldapjs": "^3.0.2",
    "memorystore": "^1.6.7",
    "morgan": "^1.10.0",
    "mysql2": "^3.9.8",
    "nocache": "^4.0.0",
    "node-cache": "^5.1.2",
    "openid-client": "^5.7.1",
    "passport": "^0.7.0",
    "prom-client": "^15.1.3"
  }
}
